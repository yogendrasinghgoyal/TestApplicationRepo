<% include adminheader %>


<section id="content">

<div class="container">
<div class="row">
<div class="span12">

<h1>Change Password Here!!!</h1>
<span style="font-size:20px; color:orange;"><%= output %></span>
<form method="post" action="/admin/cpassadmin">
<table class="table table-bordered">
<tr>
<td>Old Password</td>
<td>
<input type="hidden" name="email" value="<%= sunm %>"/>
<input style='width:100%;height:10%;' type="text" name="opass" placeholder="Enter old password"/></td>
</tr>
<tr>
<td>New Password</td>
<td><input style='width:100%;height:10%;' type="text" name="npass" placeholder="Enter new password"/></td>
</tr>
<tr>
<td>Confirm New Password</td>
<td><input style='width:100%;height:10%;' type="text" name="cnpass" placeholder="Enter confirm new password"/></td>
</tr>
<tr>
<td colspan="2"><input type="submit" value="Change Password"/></td>
</tr>
</table>
</form>


</div>
</div>


</div>
</section>


<% include adminfooter %>
